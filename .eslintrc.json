{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "plugin:jsx-a11y/recommended", "prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["jsx-a11y"],
  "rules": {
    "semi": "error",
    "no-shadow": "off",
    "react/prop-types": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
